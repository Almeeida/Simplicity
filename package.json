{
  "name": "simplicity",
  "version": "1.0.0",
  "author": "Almeida#0001 & Tsugami#2619",
  "description": "Discord Bot",
  "main": "index.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js --inspect --watch *.js --watch .env",
    "lint": "eslint src *.js",
    "lint:fix": "eslint --fix src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "license": "ISC",
  "dependencies": {
    "discord.js": "git+https://github.com/discordjs/discord.js.git#master",
    "dotenv": "^7.0.0",
    "express": "^4.17.1",
    "husky": "^3.0.9",
    "i18next": "^15.0.9",
    "i18next-node-fs-backend": "^2.1.3",
    "lint-staged": "^9.4.2",
    "module-alias": "^2.2.2",
    "moment": "^2.23.0",
    "moment-duration-format": "^2.3.2",
    "mongoose": "^5.7.1",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "eslint": "^6.4.0",
    "eslint-plugin-promise": "^4.2.1",
    "nodemon": "^1.19.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Almeeida/Simplicity.git"
  },
  "_moduleAliases": {
    "@root": "src",
    "@database": "src/database",
    "@commands": "src/commands",
    "@structures": "src/structures",
    "@utils": "src/utils",
    "@parameters": "src/parameters",
    "@loaders": "src/loaders",
    "@command": "src/structures/command",
    "@discord": "src/structures/discord",
    "@locales": "src/locales", 
    "@listeners": "src/listeners"
  }
}
