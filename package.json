{
  "name": "simplicity",
  "version": "1.0.0",
  "author": "Simplicity Team",
  "contributors": [
    "Almeida <almeida@dbm-network.org> (https://almeeida.github.io)",
    "Tsugami"
  ],
  "description": "Multiporpose Discord bot.",
  "main": "src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node .",
    "dev": "cross-env NODE_ENV=development nodemon . --watch src --watch .env",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "commit": "git-cz"
  },
  "license": "ISC",
  "dependencies": {
    "bufferutil": "^4.0.1",
    "cross-env": "^7.0.2",
    "discord.js": "github:discordjs/discord.js",
    "dotenv": "^8.2.0",
    "erlpack": "^0.1.3",
    "express": "^4.17.1",
    "i18next": "^19.4.0",
    "i18next-node-fs-backend": "^2.1.3",
    "module-alias": "^2.2.2",
    "moment": "^2.23.0",
    "moment-duration-format": "^2.3.2",
    "mongoose": "^5.9.7",
    "node-fetch": "^2.6.0",
    "source-map": "^0.7.3",
    "utf-8-validate": "^5.0.2"
  },
  "devDependencies": {
    "@types/node": "^13.11.1",
    "commitizen": "^4.0.4",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "^6.4.0",
    "eslint-config-almeida": "^1.0.7",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.2",
    "nodemon": "^2.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Almeeida/simplicity.git"
  },
  "_moduleAliases": {
    "@root": ".",
    "@src": "src",
    "@database": "src/database",
    "@commands": "src/commands",
    "@structures": "src/structures",
    "@util": "src/util",
    "@parameters": "src/structures/command/parameters/types",
    "@loaders": "src/loaders",
    "@command": "src/structures/command",
    "@discord": "src/structures/discord",
    "@locales": "src/locales",
    "@listeners": "src/listeners"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
